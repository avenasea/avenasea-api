{
  "$schema": "https://deno.land/x/denon@2.4.7/schema.json",
  "scripts": {
    "start": {
      "cmd": "deno run --allow-all --unstable index.ts",
      "desc": "run api server"
    },
    "compile": {
      "cmd": "deno compile --allow-all --unstable --output ./api index.ts",
      "desc": "compile api server"
    }
  }
}
